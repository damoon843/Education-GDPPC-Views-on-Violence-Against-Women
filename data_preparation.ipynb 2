   "source": [
    "# Check if there were multiple survey dates for each country.\n",
    "year_dict = {}\n",
    "\n",
    "for country in df['Country']:\n",
    "    year_dict[country] = df[df['Country'] == 'Afghanistan']['Survey Year'].nunique()\n",
    "\n",
    "diff_years = []\n",
    "for k in year_dict:\n",
    "    if year_dict[k] > 1:\n",
    "        diff_years.append(k)\n",
    "\n",
    "print(diff_years)"
   ]
